@CHARSET "ISO-8859-1";

body {
	margin: 0;
	padding: 0;
	text-align: center;
	background: #0092AB;
	font-family: sans-serif;
	font-size: larger;
 }

#page-wrap {
  width: 80%;
  min-width: 400px;
  margin: 0 auto;
  background-color: white;
  height: 1200px;
}

.content {
	width: 40%; 
	margin: 0 auto; 
	text-align: left; 
	min-width: 450px;
}

.header {
	background-position: center top;
	background-color: #E1E6E6;
	background-image: url("../images/AssistmentsTMBlack.png");
	height:180px;
	background-repeat: no-repeat;
}

